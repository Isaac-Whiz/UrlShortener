<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="484.0" prefWidth="582.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.urlshortener.Controller">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <Pane fx:id="panel" prefHeight="470.0" prefWidth="608.0">
      <Button fx:id="btnShortenUrl" layoutX="401.0" layoutY="109.0" mnemonicParsing="false" text="Shorten" />
      <Label fx:id="labelResult" layoutX="33.0" layoutY="174.0" prefHeight="31.0" prefWidth="360.0" text="--Shortened Url--" textFill="WHITE">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></Label>
      <TextField fx:id="txtUrl" layoutX="33.0" layoutY="52.0" prefHeight="31.0" prefWidth="426.0" promptText="Enter the Url" />
      <TextField fx:id="txtAliasUrl" layoutX="33.0" layoutY="106.0" prefHeight="31.0" prefWidth="360.0" promptText="Enter alias name for the url" />
      <Button fx:id="btnCopyUrl" layoutX="399.0" layoutY="177.0" mnemonicParsing="false" text="Copy Url" />
   </Pane>
</VBox>
